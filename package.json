  "peerDependencies": {
    "react": "16.x"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^3.0.0",
    "@semantic-release/git": "^7.0.4",
    "@semantic-release/npm": "^5.0.4",
    "@types/jest": "^23.3.2",
    "@types/react": "^16.8.4",
    "@types/react-dom": "^16.8.2",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "prettier": "^1.14.3",
    "pretty-quick": "^1.7.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-testing-library": "^5.9.0",
    "semantic-release": "^15.9.16",
    "ts-jest": "^24.0.0",
    "typescript": "^3.3.3333"
